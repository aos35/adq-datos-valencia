        <jsScript_script>// Inicializar la lista de distritos solo una vez
if (typeof listaDistritos === "undefined") {
    listaDistritos = [];
}


// Verificar que no esté duplicado (opcional)
var existe = listaDistritos.some(function(d) {
    return d.codigo === codigo_distrito;
});

if (!existe) {
    listaDistritos.push({
        codigo: codigo_distrito,
        lat: parseFloat(latitud_centroide),
        lon: parseFloat(longitud_centroide)
    });
}

function haversine(lat1, lon1, lat2, lon2) {
    var R = 6371e3; // metros
    var phi1 = lat1 * Math.PI/180;
    var phi2 = lat2 * Math.PI/180;
    var dphi = (lat2-lat1) * Math.PI/180;
    var dlambda = (lon2-lon1) * Math.PI/180;

    var a = Math.sin(dphi/2) * Math.sin(dphi/2) +
            Math.cos(phi1) * Math.cos(phi2) *
            Math.sin(dlambda/2) * Math.sin(dlambda/2);
    var c = 2 * Math.atan2(Math.sqrt(a), Math.sqrt(1-a));

    return R * c;
}

// Coordenadas de la zona
var latZona = parseFloat(latitud_zona);
var lonZona = parseFloat(longitud_zona);

var minDist = Number.MAX_VALUE;
var distritoCercano = null;

//length???
var lenLista = listaDistritos.length;

for (var i=0; i&lt;listaDistritos.length; i++) {
    var dist = haversine(latZona, lonZona, listaDistritos[i].lat, listaDistritos[i].lon);
    if (dist &lt; minDist) {
        minDist = dist;
        distritoCercano = listaDistritos[i].codigo;
    }
}

// Resultado final: cada zona recibe su distrito
codigo_distrito_zona = distritoCercano;

</jsScript_script>
